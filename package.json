{
  "name": "release2gdrive",
  "author": "Killian Hale",
  "version": "0.0.1",
  "license": "MIT",
  "bin": "./src/index.js",
  "main": "./src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "check-commit": "node ./scripts/commit-lint.js",
    "check-lock-files": "node ./scripts/check-lock-files.js",
    "copy-package-json": "node ./scripts/copy-package-json.js",
    "postinstall": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "import-sort-style-renke": "^6.0.0",
    "jest": "^28.1.1",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "semver": "^7.3.7",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx, .cjs": {
      "parser": "typescript",
      "style": "renke"
    }
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "underscore": "^1.13.4"
  }
}
